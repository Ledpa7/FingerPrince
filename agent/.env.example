SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
# Use service_role for local trusted agent. Never expose this key to frontend.
SUPABASE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY

# Optional tuning
COMMAND_TIMEOUT_SEC=120
LOG_FLUSH_INTERVAL_SEC=1.5
LOG_MAX_CHARS=20000
AGENT_USER_ID=

# IDE GUI automation (VS Code/Cursor/etc.)
# Example (VS Code): IDE_WINDOW_TITLE_SUBSTR=Visual Studio Code
IDE_WINDOW_TITLE_SUBSTR=
# Prefer hotkeys or image templates first; use coordinates only as a last resort.
# Hotkeys (examples; you must set these to YOUR VS Code keybindings if you want zero-coordinates):
IDE_OPEN_CHAT_HOTKEY=
IDE_CHAT_FOCUS_HOTKEY=
IDE_FOCUS_TRANSCRIPT_HOTKEY=
IDE_COPY_TRANSCRIPT_HOTKEY=

# Image templates (paths relative to this folder). If provided, agent will locate and click them.
IDE_INPUT_IMAGE=
IDE_OUTPUT_IMAGE=
IDE_IMAGE_TIMEOUT_SEC=4
IDE_IMAGE_CONFIDENCE=0.85
IDE_LEARN_TEMPLATE_W=320
IDE_LEARN_TEMPLATE_H=160
IDE_LEARN_COUNTDOWN_SEC=5

# Coordinates for chat input + transcript area (x,y). Use if hotkeys/images are not available.
IDE_INPUT_POS=
IDE_OUTPUT_POS=
IDE_RESPONSE_WAIT_SEC=15
# Comma-separated markers used to extract the latest assistant answer from copied transcript
AI_ANSWER_MARKERS=Assistant:,AI:,Codex:,Claude:,Cursor:,Antigravity:,답변:
